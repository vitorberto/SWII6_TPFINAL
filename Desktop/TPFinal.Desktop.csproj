    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\TPFinal.API\TPFinal.API.csproj" />
  </ItemGroup>

</Project>